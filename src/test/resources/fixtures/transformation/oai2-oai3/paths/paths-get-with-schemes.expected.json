{
  "openapi": "3.0.2",
  "servers": [
    {
      "url": "http://petstore.swagger.io/v2"
    }
  ],
  "paths": {
    "/pets": {
      "get": {
        "summary": "List Pets",
        "description": "Returns all pets from the system that the user has access to",
        "operationId": "listPets",
        "deprecated": false,
        "servers": [
          {
            "url": "{scheme}://petstore.swagger.io/v2",
            "variables": {
              "scheme": {
                "enum": [ "ws", "wss" ],
                "default": "ws",
                "description": "The supported protocol schemes."
              }
            }
          }
        ]
      }
    }
  }
}
