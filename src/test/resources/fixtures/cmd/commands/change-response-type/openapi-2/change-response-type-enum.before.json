{
  "swagger" : "2.0",
  "paths" : {
    "/pets": {
      "get": {
        "summary": "List Pets",
        "description": "Returns all pets from the system that the user has access to",
        "operationId": "listPets",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "schemes": [
          "http", "https"
        ],
        "responses": {
          "200": {
            "description": "A simple string response",
            "schema": {
              "type": "object",
              "required": [
                "name"
              ],
              "properties": {
                "name": {
                  "type": "string"
                },
                "address": {
                  "$ref": "#/definitions/Address"
                },
                "age": {
                  "type": "integer",
                  "format": "int32",
                  "minimum": 0
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "schema": {
              "$ref": "#/definitions/ErrorModel"
            }
          }
        }
      }
    }
  }
}
